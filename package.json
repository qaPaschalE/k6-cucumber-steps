{
  "name": "k6-cucumber-steps",
  "version": "2.0.10-alpha.0",
  "description": "Generate k6 test scripts from Cucumber feature files",
  "main": "dist/cli.js",
  "bin": {
    "k6-cucumber-steps": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "node scripts/generate-metadata.js",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build",
    "docs": "typedoc",
    "docs:serve": "npx http-server ./docs -p 8080 -o"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qaPaschalE/k6-cucumber-steps.git"
  },
  "bugs": {
    "url": "https://github.com/qaPaschalE/k6-cucumber-steps/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/qaPaschalE"
  },
  "homepage": "https://github.com/qaPaschalE/k6-cucumber-steps#readme",
  "keywords": [
    "k6",
    "k6 testing",
    "k6-cucumber-steps",
    "cucumber",
    "cucumber-js",
    "gherkin",
    "bdd",
    "behavior-driven development",
    "performance testing",
    "load testing",
    "stress testing",
    "api testing",
    "browser testing",
    "test automation",
    "k6 browser",
    "k6 load testing",
    "performance automation",
    "typescript testing",
    "javascript testing"
  ],
  "author": "Enyimiri Chetachi Paschal (qaPaschalE)",
  "license": "MIT",
  "dependencies": {
    "@cucumber/cucumber": "^12.6.0",
    "@cucumber/gherkin": "^26.2.0",
    "@cucumber/messages": "^23.0.0",
    "commander": "^14.0.2",
    "gherkin": "^9.0.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jquery": "^3.5.33",
    "@types/k6": "^1.5.0",
    "@types/node": "^25.0.10",
    "fs-extra": "^11.3.3",
    "typedoc": "^0.28.17",
    "typedoc-plugin-markdown": "^4.10.0"
  },
  "files": [
    "dist/**/*",
    "templates/**/*",
    "README.md",
    "LICENSE",
    "payload.json.example"
  ]
}
